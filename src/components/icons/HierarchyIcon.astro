  <line
    x1="12"
    y1="8"
    x2="12"
    y2="14"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"></line>
  <circle cx="7" cy="19" r="3" stroke="currentColor" stroke-width="2"></circle>
  <circle cx="17" cy="19" r="3" stroke="currentColor" stroke-width="2"></circle>
  <line
    x1="7"
    y1="17"
    x2="17"
    y2="17"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"></line>
</svg>
